+int
+sys_setuid32(tcp)
+struct tcb *tcp;
+{
+	if (entering(tcp)) {
+		tprintf("%u", (__kernel_uid32_t) tcp->u_arg[0]);
+	}
+	return 0;
+}
+
+int
+sys_setgid32(tcp)
+struct tcb *tcp;
+{
+	if (entering(tcp)) {
+		tprintf("%u", (__kernel_gid32_t) tcp->u_arg[0]);
+	}
+	return 0;
+}
+
+int
+sys_getresuid32(tcp)
+    struct tcb *tcp;
+{
+	if (exiting(tcp)) {
+		__kernel_uid32_t uid;
+		if (syserror(tcp))
+			tprintf("%#lx, %#lx, %#lx", tcp->u_arg[0],
+				tcp->u_arg[1], tcp->u_arg[2]);
+		else {
+			if (umove(tcp, tcp->u_arg[0], &uid) < 0)
+				tprintf("%#lx, ", tcp->u_arg[0]);
+			else
+				tprintf("ruid %lu, ", (unsigned long) uid);
+			if (umove(tcp, tcp->u_arg[0], &uid) < 0)
+				tprintf("%#lx, ", tcp->u_arg[0]);
+			else
+				tprintf("euid %lu, ", (unsigned long) uid);
+			if (umove(tcp, tcp->u_arg[0], &uid) < 0)
+				tprintf("%#lx", tcp->u_arg[0]);
+			else
+				tprintf("suid %lu", (unsigned long) uid);
+		}
+	}
+	return 0;
+}
+
+int
+sys_getresgid32(tcp)
+struct tcb *tcp;
+{
+	if (exiting(tcp)) {
+		__kernel_gid32_t gid;
